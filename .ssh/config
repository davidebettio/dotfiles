Host *
  TcpKeepAlive yes
  ServerAliveInterval 0
  ServerAliveCountMax 3
  IdentitiesOnly yes
  PubkeyAcceptedKeyTypes=+ssh-rsa
  HostKeyAlgorithms=+ssh-rsa

# bitbucket exorint
Host bitbucket.org-exorint
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes

# bitbucket davidebettio
Host bitbucket.org-davidebettio
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

